{
  "$schema": "https://vega.github.io/schema/vega-lite/v4.json",
  "data": {
    "url": "https://raw.githubusercontent.com/rodankaka/FIT3179/master-branch/Crash%20Statistics%20Victoria-V3.csv"
  },
  "width": 400,
  "height": 300,
  "encoding": {
    "x": {"timeUnit": "month", "field": "Accident Date", "title": "Month"},
    "y": {"aggregate": "sum", "field": "Fatality", "scale": {"domain": [0, 45]}, "title":"Number of Deaths"}
  },
  "layer": [
    {
      "selection": {"brush": {"type": "interval", "encodings": ["x"]}},
      "mark": "area"
    },
    {
      "transform": [{"filter": {"selection": "brush"}}],
      "mark": {"type": "area", "color": "#ff8080", "opacity": 1}
    }
  ],
  "config": {}
}
