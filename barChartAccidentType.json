{
  "$schema": "https://vega.github.io/schema/vega-lite/v4.json",
  "title": "  ",
  "data": {
    "url": "https://raw.githubusercontent.com/rodankaka/FIT3179/master-branch/Crash%20Statistics%20Victoria-V3.csv"
  },
  "width": 600,
  "height": 400,
  "mark": {"type": "bar", "tooltip": {"content": "data"}},
  "encoding": {
    "x": {
      "aggregate": "count",
      "field": "Accident Type",
      "title": "Number of Accidents",
      "sort": "ascending",
      "scale": {"domain": [0, 31000]},
      "axis": {"tickCount": 11}
      
    },
    "y": {
      "field": "Accident Type",
      "title": " ",
      "type": "nominal",
      "sort": "-x"
    },

        "tooltip": [
          {"field": "Accident Type", "type": "nominal"},
          {"field": "__count", "type": "quantitative", "title": "Number of Accidents"}
        ],

    "color": {"field": "Accident Type"}
  },
  "config": {}
}
